<blueprint
        xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
        http://www.osgi.org/xmlns/blueprint/v1.0.0
        https://osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd
">

    <reference id="jsonStore" interface="org.opennms.features.distributed.kvstore.api.JsonStore"/>

    <bean id="hcpVaultService" class="org.opennms.features.scv.vault.config.HcpVaultService" init-method="init">
        <argument ref="jsonStore"/>
    </bean>

    <service ref="hcpVaultService" interface="org.opennms.features.scv.vault.config.VaultService"/>

</blueprint>
