<?xml version="1.0" encoding="UTF-8"?>
<maps-adapter-configuration
    xmlns="http://xmlns.opennms.org/xsd/config/map/adapter" element-dimension="50">
    <package name="example1">
        <filter>IPADDR != '0.0.0.0'</filter>
        <include-range begin="1.1.1.1" end="254.254.254.254"/>
    </package>
    <cmaps>
        <cmap mapName="AllTheNetwork" mapOwner="Admin" mapAccess="RW"
            mapBG="ffffff" mapWidth="800" mapHeight="600">
            <celement icon="unspecified" package="example1"/>
        </cmap>
    </cmaps>
</maps-adapter-configuration>
