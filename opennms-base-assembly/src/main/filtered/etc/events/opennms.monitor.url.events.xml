<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
    <event>
        <uei>uei.opennms.org/monitor/url/incorrectStatusCode</uei>
        <event-label>URL Service Monitoring: Incorrect HTTP Status Code</event-label>
        <descr>&lt;p>An incorrect status code '%parm[monitorUrlStatusCode]%' was recieved for '%parm[monitorUrlSite]%'.
            args(%parm[##]%): %parm[all]%&lt;/p></descr>
        <logmsg dest="logndisplay">Incorrect HTTP Status Code '%parm[monitorUrlStatusCode]%' for '%parm[monitorUrlSite]%'.
        </logmsg>
        <severity>Major</severity>
        <alarm-data reduction-key="%uei%:%parm[monitorUrlId]%" alarm-type="1" auto-clean="false">
            <update-field field-name="severity" update-on-reduction="true"/>
        </alarm-data>
    </event>
    <event>
        <uei>uei.opennms.org/monitor/url/timeout</uei>
        <event-label>URL Service Monitoring: Timeout</event-label>
        <descr>&lt;p>Timed out fetching URL '%parm[monitorUrlSite]%'.
            args(%parm[##]%): %parm[all]%&lt;/p></descr>
        <logmsg dest="logndisplay">Timed out fetching URL '%parm[monitorUrlSite]%'.
        </logmsg>
        <severity>Minor</severity>
        <alarm-data reduction-key="%uei%:%parm[monitorUrlId]%" alarm-type="1" auto-clean="false">
            <update-field field-name="severity" update-on-reduction="true"/>
        </alarm-data>
    </event>
</events>